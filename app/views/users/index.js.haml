$('#users').html("#{j( render partial: 'user', collection: @users)}");
$('#paginate').html("#{j( paginate @users)}");

$('#search_role').val("#{@user_list}");

$('#administrators').attr('class', "#{'active' if @user_list ==  'administrator'}");
$('#subordinates').attr('class', "#{'active' if @user_list ==  'subordinate'}");
$('#secretaries').attr('class', "#{'active' if @user_list ==  'secretary'}");
