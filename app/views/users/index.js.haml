$('#users').html("#{j( render partial: 'user', collection: @users)}");
$('#paginate').html("#{j( paginate @users)}");

$('#search_role').val("#{@user_list}");
- if @user_list ==  'administrator'
  $('#administrators').attr('class', "active");
  $('#subordinates').attr('class', '');
- else
  $('#subordinates').attr('class', "active");
  $('#administrators').attr('class', '');
-#window.location.replace("users?role=#{j @user_list}");
